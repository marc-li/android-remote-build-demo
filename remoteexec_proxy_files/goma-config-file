configs: <
    dimensions: "os:win"
    remoteexec_platform: <
        rbe_instance_basename: "default_instance"
		properties: <
		    name: "container-image"
            value: "docker://${YOUR_WINDOWS_IMAGE_HERE}
         >
         properties: <
             name: "OSFamily"
             value: "Windows"
		>
    >
>

